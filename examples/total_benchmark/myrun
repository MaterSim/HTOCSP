# Print the hostname of the node executing this job
echo "Running on node: $(hostname)"
MODEL=$SLURM_JOB_NAME

python ../3-benchmark.py -p 192 -n 192 -a DFS -g 500 -d ../data/benchmark.db > log-DFS-GAFF 2>/dev/null
